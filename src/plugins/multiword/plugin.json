{
  "id": "multi.word",
  "name": "多维单词",
  "version": "0.1.0",
  "author": "LessonPlugin",
  "icon": "ri-book-2-line",
  "description": "多维单词：听力、自测、检查、外部词库、在线词典、偏好设置、关于",
  "actions": [
    { "id": "openTemplate", "icon": "ri-window-line", "text": "打开多维单词", "target": "openMultiword" }
  ],
  "variables": ["timeISO","pluginName","wordbankUrl"]
  ,"configSchema": {
    "wordbankUrl": { "type": "string", "label": "词库服务器地址", "desc": "http(s) URL", "default": "" },
    "prefs": { "type": "json", "label": "偏好设置", "desc": "{ voice, enableCarousel, shuffleAfterCarousel }", "default": { "voice": "ALL", "enableCarousel": true, "shuffleAfterCarousel": false } }
  }
}
